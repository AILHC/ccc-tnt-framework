{
    "package_version": 2,
    "version": "1.0.0",
    "name": "ccc-tnt-extensions",
    "description": "i18n:ccc-tnt-extensions.description",
    "main": "./dist/main.js",
    "dependencies": {
        "compressing": "^1.6.2",
        "download": "^8.0.0",
        "fs-extra": "^10.0.0",
        "jszip": "^3.10.1",
        "octokit": "^2.0.10",
        "vue": "^3.1.4"
    },
    "devDependencies": {
        "@types/download": "^8.0.2",
        "@types/fs-extra": "^9.0.5",
        "@types/node": "^16.0.1",
        "typescript": "^4.3.4"
    },
    "panels": {
        "default": {
            "title": "ccc-tnt-extensions Default Panel",
            "type": "dockable",
            "main": "dist/panels/default",
            "size": {
                "min-width": 400,
                "min-height": 300,
                "width": 1024,
                "height": 600
            }
        }
    },
    "contributions": {
        "menu": [
            {
                "path": "i18n:ccc-tnt-extensions.main_menu",
                "label": "生成 UI 声明文件",
                "message": "gen-ui-dts"
            },
            {
                "path": "i18n:ccc-tnt-extensions.main_menu",
                "label": "生成 Scene 声明文件",
                "message": "gen-scene-dts"
            },
            {
                "path": "i18n:ccc-tnt-extensions.main_menu",
                "label": "更新框架",
                "message": "create-template"
            }
        ],
        "messages": {
            "open-panel": {
                "methods": [
                    "openPanel"
                ]
            },
            "ccc-tnt-extensions:open": {
                "methods": [
                    "panelOpen"
                ]
            },
            "scene:ready": {
                "methods": [
                    "sceneOpen"
                ]
            },
            "gen-ui-dts": {
                "methods": [
                    "genUIDeclare"
                ]
            },
            "gen-scene-dts": {
                "methods": [
                    "genSceneDeclare"
                ]
            },
            "create-template": {
                "methods": [
                    "createTemplete"
                ]
            }
        }
    },
    "author": "onvia",
    "editor": ">=3.4.2",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    }
}
